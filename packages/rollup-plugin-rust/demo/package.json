{
  "name": "@zk-kit/hello-wasm",
  "version": "0.1.0",
  "description": "Boilerplate to compile a Rust packages to WASM.",
  "iife": "dist/index.js",
  "unpkg": "dist/index.min.js",
  "jsdelivr": "dist/index.min.js",
  "main": "dist/index.node.js",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.node.js"
  },
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "index.html",
    "LICENSE",
    "README.md"
  ],
  "repository": "git@github.com:appliedzkp/zk-kit.git",
  "homepage": "https://github.com/appliedzkp/zk-kit/tree/main/packages/hello-wasm",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "yarn build && serve",
    "build": "rimraf dist && rollup -c",
    "build:watch": "rollup -c  -w",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@zkit/rollup-plugin-rust": "../",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
